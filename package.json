{
    "name": "railsi18n",
    "displayName": "RailsI18n",
    "description": "Rails i18n support",
    "version": "0.0.1",
    "publisher": "ahmeij",
    "engines": {
        "vscode": "^1.15.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "railsi18n.showLocaleFiles",
                "title": "Show i18n files",
                "category": "Rails i18n"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.5.1",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
